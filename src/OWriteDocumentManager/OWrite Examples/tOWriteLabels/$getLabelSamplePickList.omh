If not(cvSamplePickList.$linecount)
	Do cvSamplePickList.$define(Description,ColumnName,Selected,LineFeed,Right,Bold,LongDescription)
	Do cvSamplePickList.$add("Customer Code","C_CODE",_NotSelected_,_LF_,_Right_,_Plain_,"The customers unique code")
	Do cvSamplePickList.$add("Title","C_TITLE",_Selected_,_SP_,_Left_,_Bold_,"The customers title (i.e. Mr.,Mrs.)")
	Do cvSamplePickList.$add("First Name","C_FIRST_NAME",_Selected_,_SP_,_Left_,_Bold_,"The customers first name")
	Do cvSamplePickList.$add("Other Names","C_OTHER_NAMES",_NotSelected_,_SP_,_Left_,_Bold_,"The customers other name(s)")
	Do cvSamplePickList.$add("Last Name","C_LAST_NAME",_Selected_,_LF_,_Left_,_Bold_,"The customers last name")
