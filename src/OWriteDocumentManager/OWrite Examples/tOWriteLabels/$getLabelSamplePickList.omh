	Do cvSamplePickList.$add("Address 2","C_ADDRESS_2",_Selected_,_LF_,_Left_,_Plain_,"Second line of the address (may be empty)")
	Do cvSamplePickList.$add("City/Town","C_CITY_TOWN",_Selected_,_LF_,_Left_,_Plain_,"The city or town")
	Do cvSamplePickList.$add("State/County","C_STATE_COUNTY",_Selected_,_SP_,_Left_,_Plain_,"The state or county")
