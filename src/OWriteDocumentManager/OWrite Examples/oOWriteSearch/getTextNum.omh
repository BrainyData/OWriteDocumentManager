If not(isnull(pValue))
	If not(len(pOutText))
		Calculate pOutText as con(pGroupText,' ')
	End If
	Switch pValue
		Case 0
			Calculate pOutText as con(pOutText,pVal0,', ')
		Case 1
			Calculate pOutText as con(pOutText,pVal1,', ')
		Case 2
			Calculate pOutText as con(pOutText,pVal2,', ')
		Case 3
			Calculate pOutText as con(pOutText,pVal3,', ')
	End Switch
End If